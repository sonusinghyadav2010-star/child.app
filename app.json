{
  "expo": {
    "name": "guardian-childapp",
    "slug": "guardian-child360",
    "version": "1.0.0",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a8c8c327-e0ba-4ab3-99eb-0e8e5780190e"
      }
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-router"
    ],
    "owner": "gurdianeye.app",
    "android": {
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.guardian.child",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "bundleIdentifier": "com.gurdianeye.app.guardianchild360",
      "supportsTablet": true
    }
  }
}
